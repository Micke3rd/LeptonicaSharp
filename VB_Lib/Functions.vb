	if pnarPTR <> IntPtr.Zero then pnar = new Numa(pnarPTR)
	if pnagPTR <> IntPtr.Zero then pnag = new Numa(pnagPTR)
	if pnabPTR <> IntPtr.Zero then pnab = new Numa(pnabPTR)

	Return _Result
End Function

